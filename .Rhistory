library(ggplot2)
library(dplyr)
TransgenderNGram <- read.csv("TransgenderOutput.csv")
summary(TransgenderNGram)
TransgenderNGram$n.gram <- as.factor(TransgenderNGram$n.gram)
TransgenderNGram %>%
ggplot(aes(x=n.gram,y=count)) +
geom_bar()
TransgenderNGram %>%
ggplot(aes(x=n.gram)) +
geom_bar(stat = "count")
TransgenderNGram %>%
ggplot(aes(x=n.gram, y=count)) +
geom_bar(stat = "identity")
TransgenderNGram %>%
filter(count >= 50)
TransgenderNGram %>%
filter(count >= 50) %>%
ggplot(aes(x=n.gram, y=count)) +
geom_bar(stat = "identity")
TransgenderNGram %>%
filter(count >= 50) %>%
ggplot(aes(x=n.gram, y=count)) +
geom_bar(stat = "identity") +
coord_flip()
TransgenderNGram %>%
filter(count >= 50) %>%
ggplot(aes(x=reorder(n.gram), y=count)) +
geom_bar(stat = "identity") +
coord_flip()
TransgenderNGram %>%
filter(count >= 50) %>%
ggplot(aes(x=reorder(x=n.gram), y=count)) +
geom_bar(stat = "identity") +
coord_flip()
?reorder
TransgenderNGram %>%
filter(count >= 50) %>%
ggplot(aes(x=reorder(n.gram,count), y=count)) +
geom_bar(stat = "identity") +
coord_flip()
TransgenderNGram %>%
filter(count >= 50) %>%
ggplot(aes(x=reorder(n.gram,count), y=count)) +
geom_bar(stat = "identity") +
coord_flip() +
theme_minimal()
TransgenderNGram %>%
filter(count >= 50) %>%
ggplot(aes(x=reorder(n.gram,count), y=count)) +
geom_bar(stat = "identity") +
coord_flip() +
theme_minimal() +
labs(x="Transgender + [collocate]")
TransgenderNGram %>%
filter(count >= 50) %>%
ggplot(aes(x=reorder(n.gram,count), y=count)) +
geom_bar(stat = "identity") +
coord_flip() +
theme_minimal() +
labs(x="Transgender + [collocate]", y="# of occurrences")
TransgenderNGram %>%
filter(count >= 50) %>%
ggplot(aes(x=reorder(n.gram,count), y=count)) +
geom_bar(stat = "identity") +
coord_flip() +
theme_minimal() +
labs(x="Collocation", y="# of occurrences", title="Transgender + [collocate] Counts in Breitbart Articles")
TransgenderNGram %>%
filter(count >= 50) %>%
ggplot(aes(x=reorder(n.gram,count), y=count)) +
geom_bar(stat = "identity") +
coord_flip() +
theme_minimal() +
labs(x="Collocation", y="# of occurrences", title="\"Transgender + [collocate]\" Counts in Breitbart Articles")
